<Application x:Class="fingerPrint_Manag.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:fingerPrint_Manag"
             StartupUri="Login/LogIn.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="AttendDictionary.xaml"/>
                <ResourceDictionary Source="Dictionary1.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <LinearGradientBrush x:Key="menuBrush" EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="Black" Offset="0"/>
                <GradientStop Color="#FF7F63A8" Offset="1"/>
                <GradientStop Color="#FF423357"/>
                <GradientStop Color="#FF725998" Offset="0.803"/>
                <GradientStop Color="White" Offset="0.134"/>
                <GradientStop Color="#FFD3CBDF" Offset="0.787"/>
            </LinearGradientBrush>
            <SolidColorBrush x:Key="brushMenu" Color="#FF1E1E1E"/>
        </ResourceDictionary>
    </Application.Resources>

</Application>
